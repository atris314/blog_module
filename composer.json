{
    "name": "atris314/laravel-blog-module",
    "description": "A reusable blog module for Laravel.",
    "type": "library",
    "authors": [{
            "name": "farinaz haghighi",
            "email": "farinaz.haghighi314@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Modules\\Blog\\": ""
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Blog\\Providers\\BlogServiceProvider"
            ]
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Modules\\Blog\\Tests\\": ""
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    }
}
