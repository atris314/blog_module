{
    "name": "atris314/laravel-blog-module",
    "description": "A reusable blog module for Laravel.",
    "type": "laravel module",
    "authors": [{
            "name": "farinaz haghighi",
            "email": "farinaz.haghighi314@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "Modules\\Blog\\": ""
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [],
            "aliases": {}
        },
        "laravel-modules": {
            "path": "Modules"
        }
    },


    "autoload-dev": {
        "psr-4": {
            "Modules\\Blog\\Tests\\": ""
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "atris314/laravel-blog-module": "^1.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
